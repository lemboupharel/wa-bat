{
  "name": "qasim",
  "version": "0.0.1",
  "type": "commonjs",
  "description": "A multi-functional WhatsApp bot Pairing Web and QR Scanner.",
  "main": "index.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "run": "pm2 start index.js --deep-monitoring --attach --name qasim",
    "qasim": "pm2 start index.js --deep-monitoring --attach --name qasim",
    "start": "pm2 start index.js --deep-monitoring --attach --name qasim",
    "stop": "pm2 stop qasim",
    "restart": "pm2 restart qasim",
    "reload": "pm2 reload qasim",
    "dev": "nodemon index.js",
    "clear": "pm2 stop qasim || true && rm -rf session creds.json && echo 'Session cleared. Run npm start to link again.'"
  },
  "keywords": [
    "baileysQr",
